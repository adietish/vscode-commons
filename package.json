{
  "name": "vscode-commons",
  "displayName": "Red Hat common extension",
  "description": "",
  "version": "0.0.1",
  "publisher": "redhat",
  "segmentWriteKey": "MXM7iv13sVaCGqOhnQEGLZxhfy6hecYh",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscodeCommons.clearStateAndSettings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscodeCommons.clearStateAndSettings",
        "title": "Clear context.globalState and VS Code settings"
      }
    ],
    "configuration": {
      "title": "Red Hat Telemetry",
      "properties": {
        "redhat.telemetry.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable usage data and errors to be sent to Red Hat servers",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/analytics-node": "^3.1.1",
    "@types/public-ip": "^3.1.0",
    "analytics-node": "^3.1.1"
  }
}
